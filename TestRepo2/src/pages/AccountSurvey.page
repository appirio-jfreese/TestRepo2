			}
			var spanTag = document.getElementById('spanTopSectionTotalCol2') ;
			spanTag.innerHTML = topCol2CheckboxCount ;
			updateCheckboxTotals(checkbox);
		}
		
		var topCol3CheckboxCount = -100 ;
		function updateCheckboxCountCol3(checkbox)
		{
			initializeValues();
			
			if(checkbox.checked) {
