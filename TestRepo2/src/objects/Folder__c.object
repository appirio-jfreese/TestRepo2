    <validationRules>
        <fullName>Collaboration_Section_Availability</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Page_Section__c ,  &apos;Collaboration&apos;))  &amp;&amp; ($UserRole.Name &lt;&gt; $Setup.CollaborationSectionRole__c.RoleName__c)</errorConditionFormula>
        <errorDisplayField>Page_Section__c</errorDisplayField>
        <errorMessage>You cannot add folders to Collaboration section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_allowed_only_for_Collaboration</fullName>
        <active>true</active>
        <description>Allow set comment flag only for folders which are displayed on the Collaboration page</description>
        <errorConditionFormula>AND(
    !ISPICKVAL(Page_Section__c, &apos;Collaboration&apos;),
     Allow_Add_Comment__c 
)</errorConditionFormula>
        <errorMessage>Adding comments can be enabled only for folder in a Collaboration section</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Filter_allowed_only_for_Planing</fullName>
        <active>true</active>
        <description>Allow set filter flag only for folders which are displayed on the Planing page</description>
        <errorConditionFormula>AND(
    !ISPICKVAL(Page_Section__c, &apos;Planning&apos;),
    OR(
        Filter_By_Brand__c,
        Filter_By_Time__c 
    )
)</errorConditionFormula>
        <errorMessage>Filtering by time or brand can be enabled only for folder in a Planing section</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OrganizationUnitsRequired</fullName>
        <active>true</active>
        <description>Organization Units should be not null if page section is Performance Management or Collaboration</description>
        <errorConditionFormula>( ISPICKVAL( Page_Section__c , &apos;Collaboration&apos;)  ||  ISPICKVAL( Page_Section__c , &apos;Performance Management&apos;) ) &amp;&amp; ISNULL( OrgUnits__c )</errorConditionFormula>
        <errorDisplayField>Division__c</errorDisplayField>
        <errorMessage>One or more Organization Unit should be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Uploading_allowed_only_for_Collaboration</fullName>
        <active>true</active>
        <description>Allow set upload flag only for folders which are displayed on the Collaboration page</description>
        <errorConditionFormula>AND(
    !ISPICKVAL(Page_Section__c, &apos;Collaboration&apos;),
      Allow_File_Upload__c  
)</errorConditionFormula>
        <errorMessage>Uploading files can be enabled only for folder in a Collaboration section</errorMessage>
    </validationRules>
</CustomObject>
