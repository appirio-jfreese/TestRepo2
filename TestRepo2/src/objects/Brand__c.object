    <validationRules>
        <fullName>Parent_Brand</fullName>
        <active>true</active>
        <description>The Parent Brand cannot be changed once the Brand is created since the Magento API sync call does not support updating the ParentId of a Category</description>
        <errorConditionFormula>ISCHANGED(Parent_Brand__c)</errorConditionFormula>
        <errorDisplayField>Parent_Brand__c</errorDisplayField>
        <errorMessage>The Parent Brand cannot be changed once the Brand is created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Portfolio_Brand_Creation</fullName>
        <active>true</active>
        <description>Prevents a new portfolio brand from being created</description>
        <errorConditionFormula>(PRIORVALUE(Portfolio_Brand__c) = FALSE) &amp;&amp; (Portfolio_Brand__c = TRUE)</errorConditionFormula>
        <errorMessage>Only one brand can be marked as &quot;Portfolio Brand&quot;</errorMessage>
    </validationRules>
