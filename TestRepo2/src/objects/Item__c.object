    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>Kit_Only_Item</fullName>
        <active>true</active>
        <description>Enforces the record type to be &quot;Standard&quot; if the &quot;Kit Only&quot; field is checked</description>
        <errorConditionFormula>Kit_Only__c  &amp;&amp; $RecordType.Name &lt;&gt; &quot;Standard&quot;</errorConditionFormula>
        <errorDisplayField>Kit_Only__c</errorDisplayField>
        <errorMessage>A &quot;Kit Only&quot; item must be a &quot;Standard&quot; item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Pricing_Information</fullName>
        <active>true</active>
        <description>Pricing information cannot be manually edited by CM Administrators or POS Agencies once the final pricing of an item is accepted</description>
        <errorConditionFormula>($Profile.Name = &quot;POS - Production Agency&quot; || $Profile.Name = &quot;POS - CM Administrator&quot;) &amp;&amp; (TEXT(Status__c) = &quot;Accepted w/Final Price&quot;) &amp;&amp; (ISCHANGED(Current_Price__c) || ISCHANGED(Estimated_Price__c) || ISCHANGED(Estimated_Shipping_Tax__c))</errorConditionFormula>
        <errorMessage>The pricing information cannot be changed since this item is accepted with final price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Parent_Item_Change</fullName>
        <active>true</active>
        <description>The Parent Item cannot be changed once the Item is created since the Magento API sync call does not support updating the Item&apos;s parent id</description>
        <errorConditionFormula>ISCHANGED(Parent__c)</errorConditionFormula>
        <errorDisplayField>Parent__c</errorDisplayField>
        <errorMessage>The Parent Item cannot be changed once the Item is created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_RecordType_Change</fullName>
        <active>true</active>
        <description>Prevents an Item&apos;s record type from being changed</description>
        <errorConditionFormula>ISCHANGED(RecordTypeId)</errorConditionFormula>
        <errorMessage>The Item Record Type cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Agency</fullName>
        <active>true</active>
        <description>Enforces the Item to have a related Agency. Intended for integration via API.</description>
        <errorConditionFormula>ISBLANK(Agency__c)</errorConditionFormula>
        <errorDisplayField>Agency__c</errorDisplayField>
        <errorMessage>Please provide a value for the Agency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Category</fullName>
        <active>true</active>
        <description>Enforces the Item to have a Category. Intended for integration via API.</description>
        <errorConditionFormula>ISPICKVAL(Category__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Category__c</errorDisplayField>
        <errorMessage>Please provide a value for the Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Description</fullName>
        <active>true</active>
        <description>Enforces the Item to have a Description. Intended for integration via API.</description>
        <errorConditionFormula>ISBLANK(Description__c)</errorConditionFormula>
        <errorDisplayField>Description__c</errorDisplayField>
        <errorMessage>Please provide a value for the Description</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Item_Category</fullName>
        <active>true</active>
        <description>Enforces the Item to have a related Item Category. Intended for integration via API.</description>
        <errorConditionFormula>ISBLANK(Item_Category__c)</errorConditionFormula>
        <errorDisplayField>Item_Category__c</errorDisplayField>
        <errorMessage>Please provide a value for the Item Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Program</fullName>
        <active>true</active>
        <description>Please provide a value for the Program</description>
        <errorConditionFormula>ISBLANK(Program__c)</errorConditionFormula>
        <errorDisplayField>Program__c</errorDisplayField>
        <errorMessage>Enforces the Item to have a related Program. Intended for integration via API.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Status</fullName>
        <active>true</active>
        <description>Enforces the Item to have a Status. Intended for integration via API.</description>
        <errorConditionFormula>ISPICKVAL(Status__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please provide a value for the Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Incremental_Charges</fullName>
        <active>true</active>
        <description>Enforces Valid Incremental Charges</description>
        <errorConditionFormula>Incremental_Charges__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Incremental_Charges__c</errorDisplayField>
        <errorMessage>Please provide a valid amount for incremental charges</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Minimum_Quantity</fullName>
        <active>true</active>
        <description>Enforces a valid minimum quantity</description>
        <errorConditionFormula>(ISBLANK(Minimum_Quantity__c) || Minimum_Quantity__c &lt; 1) &amp;&amp; (Kit_Only__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Minimum_Quantity__c</errorDisplayField>
        <errorMessage>Please provide a valid minimum quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Packout_Quantity</fullName>
        <active>true</active>
        <description>Enforces a valid packout quantity</description>
        <errorConditionFormula>(ISBLANK(Packout_Quantity__c) || Packout_Quantity__c &lt; 1) &amp;&amp; (Kit_Only__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Packout_Quantity__c</errorDisplayField>
        <errorMessage>Please provide a valid packout quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Pricing_Information</fullName>
        <active>true</active>
        <description>Enforces valid pricing information for non kit-only items: current price, estimated price and estimated shipping/tax when an Item is not a kit-only Item</description>
        <errorConditionFormula>(ISBLANK(Current_Price__c) || ISBLANK(Estimated_Price__c) || ISBLANK(Estimated_Shipping_Tax__c) || Current_Price__c &lt; 0 || Estimated_Price__c &lt; 0 || Estimated_Shipping_Tax__c &lt; 0) &amp;&amp; (Kit_Only__c = FALSE)</errorConditionFormula>
        <errorMessage>Please provide valid pricing information (Current Price, Estimated Price and Estimated Shipping/Tax %)</errorMessage>
    </validationRules>
