    <webLinks>
        <fullName>Deep_Clone</fullName>
        <availability>online</availability>
        <description>Used with a webservice call (plus a trigger) to run a deep clone (with child-lookup records cloned as well) of a given survey layout record.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Deep Clone</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/apex.js&quot;)}


var recordId = &apos;{!Survey_Layout__c.Id}&apos; ;


if (recordId) {
     var methodArgumentsObject = {} ;
     methodArgumentsObject.surveyLayoutId = recordId ;
     var result = sforce.apex.execute( &quot;WebServiceCloneSurvey&quot;, &quot;cloneSurveyLayoutRecordAndReturnMessageString&quot;, 
                                                                    methodArgumentsObject );
     var resultString = result[0] ;
     if(resultString.substr(0,8) == &apos;SUCCESS-&apos;) {
          var url = &apos;/&apos; ;
          url += resultString.substr(8) ;
          window.location.href = url ;
     }else{
          alert(resultString);
     }

}</url>
    </webLinks>
